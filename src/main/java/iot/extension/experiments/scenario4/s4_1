reset
set xlabel 'Providers' font 'Times,12'
set ylabel 'Price in Euro in logarithmic scale' font 'Times,12'
set logscale y
set style data histograms
set style histogram cluster gap 1
set boxwidth 0.7 relative
set style fill pattern 1.2 border -1
set xtic 1 offset character 0,0 font 'Times,12'
set label "3623.28" at -0.23,4500
set label "421.65" at 0.8,550
set label "26.63" at 1.85,35
set label "0.259" at 2.87,0.33
set grid ytics
set  logscale y
plot 's4_1_data' using 1:xtic(2) notitle lc rgb "#FF0000"
