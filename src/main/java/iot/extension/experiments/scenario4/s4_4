reset
set bmargin 6
set xlabel 'Category of the virtual machine' font 'Times,30' offset 0,-1,0
set ylabel 'Number of virtual machines / tasks in logarithmic scale' font 'Times,30' offset -4,0,0
set style data histograms
set style histogram gap 1
set boxwidth 0.7 relative
#set yrange[0 : 80]
set grid ytics
set logscale y
set style fill pattern 1.2 border -1
set key font 'Times,20'
set ytic font 'Times,20'
set xtic font 'Times,20' offset 0,0,0
plot 's4_4_data' using 2 :xtic(1)  ti "Virtual machines" lc rgb "#FF0000" ,\
'' using 3 ti "Tasks" lc rgb "#0000FF" 

