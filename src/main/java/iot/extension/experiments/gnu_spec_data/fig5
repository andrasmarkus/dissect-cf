reset
set ylabel 'Number of tasks' font 'Times,12'
set xlabel 'Time in ms' font 'Times,12'
set datafile sep ','
set grid ytics
set yrange [0:8]
plot 'tasks487.csv'u 1:2 w l lw 3 lt rgb "#FF0000" dashtype 2 title "interval 1min", 'tasks487_5.csv' u 1:2 w l lw 3  lt rgb "#0000FF" dashtype 1 title 'interval 5min'
