reset
set ylabel 'Number of tasks' font 'Times,12'
set xlabel 'Time in ms' font 'Times,12'
set datafile sep ','
set grid ytics
set yrange [0:25]
set xrange [0:5400000]
plot 'tasks200.csv'u 1:2 w l lw 3 lt rgb "#00FF00" dashtype 2 title "200 bytes",'tasks100.csv' u 1:2 w l lw 3  lt rgb "#FF0000" dashtype 1 title "100 bytes",'tasks50.csv' u 1:2 w l lw 3  lt rgb "#0000FF" dashtype 3 title "50 bytes"
